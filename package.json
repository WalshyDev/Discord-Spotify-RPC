{
  "name": "discord-spotify-rpc",
  "version": "1.1.0",
  "description": "Adds Spotify Rich Presence to Discord",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WalshyDev/Discord-Spotify-RPC.git"
  },
  "scripts": {
    "start": "node src/index.js",
    "postinstall": "node scripts/checkNodeVersion.js",
    "build": "pkg --out-path build/ -t node8-linux-x64,node8-macos-x64,node8-win-x64 . && cp config.json build/config.json"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "discord-rpc": "^3.0.0-beta.3",
    "node-spotify-webhelper": "https://github.com/liangchunn/node-spotify-webhelper"
  },
  "devDependencies": {
    "pkg": "^4.3.0-beta.1",
    "semver": "^5.4.1"
  },
  "keywords": [
    "discord",
    "spotify",
    "rpc"
  ],
  "author": "WalshyDev",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/WalshyDev/Discord-Spotify-RPC/issues"
  },
  "homepage": "https://github.com/WalshyDev/Discord-Spotify-RPC#readme",
  "pkg": {
    "scripts": "src/**/*.js"
  },
  "bin": "src/index.js"
}
